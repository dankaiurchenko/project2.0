<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane stylesheets="@style.css" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tischenko.controllers.TokensController">
    <TableView fx:id="tokensTable" layoutX="87.0" layoutY="52.0" prefHeight="400.0" prefWidth="600.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="tokenNumberColumn" minWidth="30.0" prefWidth="-1.0" text="N"/>
          <TableColumn fx:id="tokenLineColumn" minWidth="30.0" prefWidth="-1.0" text="Line"/>
          <TableColumn fx:id="tokenTokenColumn" editable="false" minWidth="60.0" prefWidth="-1.0" text="Token"/>
          <TableColumn fx:id="tokenCodeColumn" minWidth="30.0" prefWidth="-1.0" text="Code"/>
          <TableColumn fx:id="tokenConstantCodeColumn" minWidth="30.0" prefWidth="-1.0" text="Const N"/>
          <TableColumn fx:id="tokenIdentifierCodeColumn" minWidth="30.0" prefWidth="-1.0" text="ID N"/>
        </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>

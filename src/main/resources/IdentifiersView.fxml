<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane stylesheets="@style.css" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tischenko.controllers.IdentifiersController">
    <TableView fx:id="identifiersTable" layoutX="10.0" layoutY="10.0" prefHeight="300.0" prefWidth="350.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="identifierNumberColumn" minWidth="30.0" prefWidth="-1.0" text="N"/>
          <TableColumn fx:id="identifierNameColumn" editable="false" minWidth="60.0" prefWidth="-1.0" sortable="false"
                       text="ID"/>
          <TableColumn fx:id="identifierTypeColumn" minWidth="40.0" prefWidth="-1.0" text="Type"/>
        </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
